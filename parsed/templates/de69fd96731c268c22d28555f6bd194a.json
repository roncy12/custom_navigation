{
  "components/common/header": "<a href=\"#main-content\" class=\"skip-to-main-link\">{{lang 'header.skip_to_main'}}</a>\r\n{{#if banners.top}}\r\n<div class=\"banners\" data-banner-location=\"top\">\r\n    {{#each (limit banners.top_metadata 1)}}\r\n    <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-banner-id=\"{{this.banner-name}}\">\r\n        <div data-event-type=\"promotion-click\">\r\n            {{{this.content}}}\r\n        </div>\r\n    </div>\r\n    {{/each}}\r\n</div>\r\n\r\n\r\n{{/if}}\r\n\r\n<header class=\"header header-bannerimg\" id=\"myHeader\" role=\"banner\">\r\n    <div class=\"header-div \">\r\n    <div>\r\n        <a href=\"#\" class=\"mobileMenu-toggle\" data-mobile-menu-toggle=\"menu\">\r\n             <span class=\"mobileMenu-toggleIcon\">Toggle menu</span>\r\n         </a> \r\n    </div>\r\n    <div class=\"links-header logo-is\">\r\n        {{#if page_type '===' 'default'}}\r\n        <div class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\r\n            {{> components/common/store-logo}}\r\n        </div>\r\n        {{else}}\r\n            <div class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\r\n                {{> components/common/store-logo}}\r\n            </div>\r\n        {{/if}}\r\n    </div>\r\n\r\n\r\n    <div class=\"links-header head-nav\">\r\n         <div class=\"navPages-container\" id=\"menu\" data-menu>\r\n            {{> components/common/navigation-menu}}\r\n        </div>\r\n    </div>\r\n\r\n    <div class=\"links-header head-cart-icon\">\r\n      {{> components/common/navigation}}\r\n    </div>\r\n</div>\r\n   \r\n</header>\r\n<!-- {{{region name=\"header_bottom--global\"}}}\r\n{{{region name=\"header_bottom\"}}} -->\r\n",
  "components/common/store-logo": "<a href=\"{{urls.home}}\" class=\"header-logo__link\">\r\n    {{#if settings.store_logo.image}}\r\n        {{#if theme_settings.logo_size '===' 'original'}}\r\n            <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n        {{else}}\r\n            <div class=\"header-logo-image-container footer-logo\">\r\n                <img class=\"header-logo-image footer-logo\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n            </div>\r\n        {{/if}}\r\n    {{else}}\r\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\r\n    {{/if}}\r\n</a>\r\n",
  "components/common/navigation-menu": "<nav class=\"navPages\">\r\n    <div class=\"navPages-quickSearch\">\r\n        {{> components/common/quick-search name='nav-menu-quick-search'}}\r\n    </div>\r\n    <ul class=\"navPages-list{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}}\">\r\n        <!-- {{#each categories}}\r\n            <li class=\"navPages-item\">\r\n            {{#if ../theme_settings.navigation_design '===' 'simple'}}\r\n                {{> components/common/navigation-list}}\r\n            {{/if}}\r\n            {{#if ../theme_settings.navigation_design '===' 'alternate'}}\r\n                {{> components/common/navigation-list-alternate}}\r\n            {{/if}}\r\n            </li>\r\n        {{/each}} -->\r\n        <!-- {{#unless theme_settings.hide_content_navigation}}\r\n            {{#each pages}}\r\n                 <li class=\"navPages-item navPages-item-page\">\r\n                     <a class=\"navPages-action{{#if name '==' ../page.title}} activePage{{/if}}\"\r\n                        href=\"{{url}}\"\r\n                        aria-label=\"{{name}}\"\r\n                     >\r\n                         {{name}}&nbsp;\r\n                     </a>\r\n                 </li>\r\n             {{/each}}\r\n        {{/unless}} -->\r\n        <li class=\"navPages-item navPages-item-page\">\r\n            <a class=\"navPages-action \"\r\n               href=\"/green-tea-memory-foam-bed-mattres/\"\r\n               aria-label=\"{{name}}\">\r\n                Shop Our Mattress&nbsp;\r\n            </a>\r\n        </li>\r\n        <li class=\"navPages-item navPages-item-page\">\r\n            <a class=\"navPages-action \"\r\n               href=\"/bases/\"\r\n               aria-label=\"{{name}}\">\r\n                Bases&nbsp;\r\n            </a>\r\n        </li>\r\n        <li class=\"navPages-item navPages-item-page\">\r\n            <a class=\"navPages-action \"\r\n               href=\"/bedding/\"\r\n  